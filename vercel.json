{
  "version": 2,
  "builds": [
    {
      "src": "web_service/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web_service/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11.6"
  },
  "functions": {
    "web_service/app.py": {
      "maxDuration": 30
    }
  }
}